<!DOCTYPE html>
<html>
  <head>
    <title>File Upload</title>
    <style>
      body {
        font-family: sans-serif;
      }

      .container {
        width: 300px;
        margin: 0 auto;
      }

      .file-upload {
        border: 1px solid #ccc;
        padding: 10px;
      }

      .file-upload input {
        width: 100%;
      }

      .file-upload button {
        background-color: #000;
        color: #fff;
        padding: 10px;
        cursor: pointer;
      }
    </style>
  </head>
  <body>
    <div class="container">
      <form action="/upload" method="POST" enctype="multipart/form-data">
        <h1>File Upload</h1>
        <div class="file-upload">
          <label for="file">Uplaod file here</label>
          <input type="file" name="file" id="file" />
          <input type="submit" value="Submit" />
        </div>
      </form>
    </div>
    <!-- <script src="/script/form.js"></script> -->
  </body>
</html>
